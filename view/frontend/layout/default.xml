<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block ifconfig="structureddata/cms/enable" class="OuterEdge\StructuredData\Block\Jsonld" name="structured.data.jsonld" template="OuterEdge_StructuredData::jsonld.phtml"/>
        </referenceBlock>
        <referenceBlock name="breadcrumbs">
            <action method="setTemplate">
                <argument name="template" xsi:type="string">OuterEdge_StructuredData::html/breadcrumbs.phtml</argument>
            </action>
        </referenceBlock>
    </body>
</page>
